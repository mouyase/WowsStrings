msgid "IDS_DESC_PCM064_TORPEDOBOMBER_MOD_I"
msgstr "Aumenta los PV de los bombarderos torpederos"

msgid "IDS_DESC_PCM065_DIVEBOMBER_MOD_I"
msgstr "Aumenta los PV de los bombarderos"

msgid "IDS_DESC_PCM066_TORPEDOBOMBER_MOD_II"
msgstr "Aumenta el tiempo de ataque de los bombarderos torpederos"

msgid "IDS_DESC_PCM067_FIGHTER_MOD_II"
msgstr "Aumenta el tiempo de ataque de los aviones de ataque"

msgid "IDS_DESC_PCM068_PLANEENGINE_MOD_I"
msgstr ""
"Aumenta el tiempo del empuje del motor para los aviones del portaaviones"

msgid "IDS_DESC_PCM069_MOVEMENT_MOD_I"

msgid "IDS_PCEE360_BRANDENBURG_FLAG_EXTRA_DESCRIPTION"
msgstr " "

msgid "IDS_PCEE361_FLORIDA_FLAG"
msgstr "Florida"

msgid "IDS_PCEE361_FLORIDA_FLAG_DESCRIPTION"
msgstr ""

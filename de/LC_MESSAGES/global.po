msgid "IDS_000_001_BEWARE_RASPUTIN_WARNING"
msgstr ""
"Zerstören Sie Rasputin!\n"
"Die Transylvania darf nicht zerstört erden!"

msgid "IDS_000_001_MAIN_TASK_BRIEFING"
msgstr ""
"Begleiten Sie die Transylvania zum Portal\n"
"und schützen Sie sie."

msgid "IDS_000_001_MAIN_TASK_TASK_END_F"
msgstr ""
"Unternehmen fehlgeschlagen.\n"
"Die Transylvania wurde zerstört."

msgid "IDS_000_001_MAIN_TASK_TASK_END_S"
msgstr ""
"Unternehmen abgeschlossen!\n"
"Die Transylvania hat ihr Ziel erreicht."

msgid "IDS_000_001_MAIN_TASK_TASK_WILL_END_F"
msgstr ""
"Unternehmen fehlgeschlagen.\n"
"Alle Verteidiger wurden zerstört."

msgid "IDS_000_001_ZEEKASA_WARNING"
msgstr ""
"Drängen Sie die feindlichen Schiffe zurück und beschützen Sie die "
"Transylvania!"

msgid "IDS_000_011_MAIN_TASK_HINT1"
msgstr ""
"Die Transylvania läuft schneller, wenn mehrere verbündete Schiffe in der "
"Nähe sind."

msgid "IDS_000_011_MAIN_TASK_HINT2"
msgstr "Die Transylvania kommt zum Stehen, wenn ihr Zombies zu nahe kommen."

msgid "IDS_000_021_PAYLOAD_HALF_HEALTH_TASK_END_F"
msgstr "Massive Schäden erlitten. Die Rettungsoperation ist in Gefahr!"

msgid "IDS_000_022_KILL_ALL_ENEMIES_TASK_END_S"
msgstr ""
"Wann werden die weniger? Das ist der sechzehnte Gegner, den wir zerstört "
"haben!"

msgid "IDS_000_024_UNSTOPPABLE_TASK_END_F_ENEMY_IS_COMING"
msgstr "Stoppen Sie die Motoren! Der Gegner ist zu nahegekommen!"

msgid "IDS_000_024_UNSTOPPABLE_TASK_END_F_NO_FRIENDS"
msgstr "Stoppen Sie die Motoren! Die Verbündeten sind zu weit weg!"

msgid "IDS_000_026_ALL_ALIVE_TASK_END_F"
msgstr "Der erste Verbündete wurde zerstört, wir verlieren."


msgid "IDS_PCEE358_DECEPTICONS_FLAG_DESCRIPTION"
msgstr "Un vessillo ai potenti Decepticon."

msgid "IDS_PCEE358_DECEPTICONS_FLAG_EXTRA_DESCRIPTION"

msgstr "Modificação do Sistema de Controle de Tiros do Canhão 1"

msgid "IDS_TITLE_PCM009_FLIGHTCONTROL_MOD_I"
msgstr "Modificação de Controle Aéreo 1"

msgid "IDS_TITLE_PCM010_AIRPLANES_MOD_II"
msgstr "Modificação dos Grupos Aéreos 2"

msgid "IDS_TITLE_PCM011_AIRDEFENSE_MOD_II"
msgstr "Modificação dos Canhões AA 2"

msgid "IDS_TITLE_PCM012_SECONDARYGUN_MOD_II"
msgstr "Modificação da Bateria Secundária 1"

msgid "IDS_TITLE_PCM013_MAINGUN_MOD_III"
msgstr "Modificação da Bateria Principal 3"

msgid "IDS_TITLE_PCM014_TORPEDO_MOD_III"
msgstr "Modificação dos Tubos de Torpedos 2"

msgid "IDS_TITLE_PCM015_FIRECONTROL_MOD_II"
msgstr "Modificação do Sistema de Controle de Tiros do Canhão 2"

msgid "IDS_TITLE_PCM016_FLIGHTCONTROL_MOD_II"
msgstr "Modificação de Controle Aéreo 2"

msgid "IDS_TITLE_PCM017_AIRPLANES_MOD_III"
msgstr "Modificação dos Grupos Aéreos 2"

msgid "IDS_TITLE_PCM018_AIRDEFENSE_MOD_III"
msgstr "Modificação dos Canhões AA 1"

msgid "IDS_TITLE_PCM019_SECONDARYGUN_MOD_III"
msgstr "Modificação da Bateria Secundária 2"

msgid "IDS_TITLE_PCM020_DAMAGECONTROL_MOD_I"
msgstr "Modificação do Sistema de Controle de Danos 1"

msgid "IDS_TITLE_PCM021_ENGINE_MOD_I"
msgstr "Modificação de Propulsão 1"

msgid "IDS_TITLE_PCM022_STEERINGGEAR_MOD_I"
msgstr "Modificação da Caixa de Direção 1"

msgid "IDS_TITLE_PCM023_DAMAGECONTROL_MOD_II"
msgstr "Modificação do Sistema de Controle de Danos 2"

msgid "IDS_TITLE_PCM024_ENGINE_MOD_II"
msgstr "Modificação de Propulsão 1"

msgid "IDS_TITLE_PCM025_STEERINGGEAR_MOD_II"
msgstr "Modificação da Caixa de Direção 1"

msgid "IDS_TITLE_PCM026_LOOKOUTSTATION_MOD_I"
msgstr "Sistema de Vigilância de Torpedos"

msgid "IDS_TITLE_PCM027_CONCEALMENTMEASURES_MOD_I"
msgstr "Modificação do Sistema de Ocultamento 1"

msgid "IDS_TITLE_PCM028_FIRECONTROL_MOD_I_US"
msgstr "Modificação 1 da Sala de Plotagem da Artilharia"

msgid "IDS_TITLE_PCM029_FIRECONTROL_MOD_II_US"
msgstr "Modificação 2 da Sala de Plotagem da Artilharia"

msgid "IDS_TITLE_PCM030_MAINWEAPON_MOD_I"
msgstr "Modificação dos Armamentos Principais 1"

msgid "IDS_TITLE_PCM031_SECONDARYWEAPON_MOD_I"
msgstr "Modificação dos Armamentos Auxiliares 1"

msgid "IDS_TITLE_PCM032_POWDERMAGAZINE_MOD_I"
msgstr "Modificação do Paiol 1"

msgid "IDS_TITLE_PCM033_GUIDANCE_MOD_I"
msgstr "Modificação dos Sistemas de Mira 1"

msgid "IDS_TITLE_PCM034_GUIDANCE_MOD_0"
msgstr "Modificação dos Sistemas de Mira 0"

msgid "IDS_TITLE_PCM035_STEERINGGEAR_MOD_III"
msgstr "Modificação da Caixa de Direção 2"


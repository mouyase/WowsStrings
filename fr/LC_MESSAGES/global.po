msgid "IDS_000_001_BEWARE_RASPUTIN_WARNING"
msgstr ""
"Détruisez le Rasputin !\n"
"Ne le laissez pas dévaster le Transylvania !"

msgid "IDS_000_001_MAIN_TASK_BRIEFING"
msgstr ""
"Escortez le Transylvania jusqu'au portail\n"
"en le protégeant de tous les dangers !"

msgid "IDS_000_001_MAIN_TASK_TASK_END_F"
msgstr ""
"Échec de l'opération.\n"
"Le Transylvania a été détruit."

msgid "IDS_000_001_MAIN_TASK_TASK_END_S"
msgstr ""
"Réussite de l'opération.\n"
"Le Transylvania a atteint sa destination."

msgid "IDS_000_001_MAIN_TASK_TASK_WILL_END_F"
msgstr ""
"Échec de l'opération.\n"
"Tous les défenseurs ont été détruits."

msgid "IDS_000_001_ZEEKASA_WARNING"
msgstr "Repoussez les navires ennemis et protégez le Transylvania !"

msgid "IDS_000_011_MAIN_TASK_HINT1"
msgstr ""
"Le Transylvania se déplace plus vite si plusieurs navires alliés se "
"trouvent à proximité."

msgid "IDS_000_011_MAIN_TASK_HINT2"
msgstr ""
"Le Transylvania s'arrête lorsque des zombies se rapprochent trop de lui !"

msgid "IDS_000_021_PAYLOAD_HALF_HEALTH_TASK_END_F"
msgstr "Dégâts considérables reçus. Opération de sauvetage en danger !"

msgid "IDS_000_022_KILL_ALL_ENEMIES_TASK_END_S"
msgstr ""
"Quand en aurons-nous fini ? C'est le seizième ennemi que nous avons oblité"
"ré !"

msgid "IDS_000_024_UNSTOPPABLE_TASK_END_F_ENEMY_IS_COMING"
msgstr "Arrêtez les moteurs ! l'ennemi est trop proche !"

msgid "IDS_000_024_UNSTOPPABLE_TASK_END_F_NO_FRIENDS"
msgstr "Arrêtez les moteurs ! Les alliés sont trop loin."

msgid "IDS_000_026_ALL_ALIVE_TASK_END_F"
msgstr "Le premier allié a été détruit, nous sommes en train de perdre."


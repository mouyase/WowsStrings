msgid "IDS_000_001_BEWARE_RASPUTIN_WARNING"
msgstr ""
"Zničte Rasputina!\n"
"Nenechte ho zničit Transylvánii!"

msgid "IDS_000_001_MAIN_TASK_BRIEFING"
msgstr ""
"Eskortujte Transylvánii k portálu\n"
"a chraňte ji před jakýmkoliv poškozením!"

msgid "IDS_000_001_MAIN_TASK_TASK_END_F"
msgstr ""
"Operace selhala.\n"
"Transylvánie byla zničena."

msgid "IDS_000_001_MAIN_TASK_TASK_END_S"
msgstr ""
"Operace dokončena!\n"
"Transylvánie se dostala k cíli."

msgid "IDS_000_001_MAIN_TASK_TASK_WILL_END_F"
msgstr ""
"Operace selhala.\n"
"Všichni obránci byli zničeni."

msgid "IDS_000_001_ZEEKASA_WARNING"
msgstr "Braňte se proti nepřátelským lodím a chraňte Transylvánii!"

msgid "IDS_000_011_MAIN_TASK_HINT1"
msgstr ""
"Transylvánie se pohybuje rychleji, pokud je poblíž několik spojeneckých "
"lodí."

msgid "IDS_000_011_MAIN_TASK_HINT2"
msgstr ""
"Transylvánie se zastaví, když se k ní zombie dostanou příliš blízko!"

msgid "IDS_000_021_PAYLOAD_HALF_HEALTH_TASK_END_F"
msgstr "Obdrženo rozsáhlé poškození. Záchranná operace je ohrožena."

msgid "IDS_000_022_KILL_ALL_ENEMIES_TASK_END_S"
msgstr ""
"Kdy to skončí? To je šestnáctý nepřítel, kterého jsme zničili!"

msgid "IDS_000_024_UNSTOPPABLE_TASK_END_F_ENEMY_IS_COMING"
msgstr "Zastavte stroj! Nepřítel je příliš blízko!"

msgid "IDS_000_024_UNSTOPPABLE_TASK_END_F_NO_FRIENDS"
msgstr "Zastavte stroj! Spojenci se přesunuli příliš daleko."

msgid "IDS_000_026_ALL_ALIVE_TASK_END_F"
msgstr "První spojenec byl zničen. Prohráváme."


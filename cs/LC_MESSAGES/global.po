msgstr "Úprava pozorovacího letadla 1"

msgid "IDS_TITLE_PCM039_CRASHCREW_MOD_I"
msgstr "Úprava havarijního týmu 1"

msgid "IDS_TITLE_PCM040_AIRDEFENSEDISP_MOD_I"
msgstr "Úprava obranné protiletadlové palby 1"

msgid "IDS_TITLE_PCM041_SONARSEARCH_MOD_I"
